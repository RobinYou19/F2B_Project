#!/home/robin/Dev/Project/xaal_env/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'coloredlogs','console_scripts','coloredlogs'
__requires__ = 'coloredlogs'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('coloredlogs', 'console_scripts', 'coloredlogs')()
    )
